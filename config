cmd_path=/usr/local/bin/mosquitto_sub
cmd_path_server=/usr/local/sbin/mosquitto
cmd_confpath_server=/usr/local/etc/mosquitto/mosquitto.conf
cmd_confpath=/root/.config/mosquitto_sub
cmd_ipaddr=localhost
cmd_ipport=1883
mqtt_topics=-t "#"
cmd_args_prefix=-R
cmd_args_postfix=
cmd_timeout=5
pid_file=/var/run/mosquitto.pid
show_conf=0
show_advanced=0
allow_cmdexec=0
show_cmd=0
mqtt_username=
mqtt_password=
start_cmd=service mosquitto start
stop_cmd=service mosquitto stop
restart_cmd=service mosquitto restart
